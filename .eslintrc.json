{
  "env": {
    "browser": true,
    "es2021": true,
    "jest": true
  },
  "extends": [
    "airbnb-base",
    "prettier"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": [
    "@typescript-eslint"
  ],
  "rules": {
    "camelcase": "off",
    "no-console": "off",
    "lines-between-class-members": "off",
    "no-trailing-spaces": "off",
    "quotes": "off",
    "import/no-unresolved": "off",
    "import/extensions": "off"
  }
}
